{ 
  "name": "mood-menu",
  "version": "0.2.0",
  "private": true,
  "description": "An AI-powered weird food suggestion app that matches chaotic dishes to your mood",
  "homepage": "https:github.com/sanjay-sanju-03/Mood-Menu",
  "dependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "face-api.js": "^0.20.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-scripts": "^5.0.1",
    "react-webcam": "^7.2.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
